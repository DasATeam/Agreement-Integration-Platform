<!DOCTYPE html>
<html>
	<head>
		
	</head>
	<body>

		<%= form_for :merchant do |f| %>
		  <p>
		    <%= f.label :userid %><br>
		    <%= f.text_field :userid%>
		  </p>
		  <p>
		    <%= f.label :name %><br>
		    <%= f.text_field :name%>
		  </p>
		  <p>
		    <%= f.label :website %><br>
		    <%= f.text_field :website%>
		  </p>
		  <p>
		    <%= f.label :email %><br>
		    <%= f.text_field :email%>
		  </p>
		  <p>
		    <%= f.label :city %><br>
		    <%= f.text_field :city%>
		  </p>
		  <p>
		    <%= f.label :address %><br>
		    <%= f.text_field :address%>
		  </p>
		  <p>
		    <%= f.label :officestatus %><br>
		    <%= options_for_select([['Milik Sendiri', 1], ['Milik Keluarga', 2], ['Kontrak', 3]], 1) %>
		  </p>
		  <p>
		    <%= f.label :typeofproduct %><br>
		    <%= f.text_area :typeofproduct %>
		  </p>
		  <p>
		    <%= f.label :pricerange %><br>
		    <%= f.text_area :pricerange %>
		  </p>
		  <p>
		    <%= f.label :timesincestarted %><br>
		    <%= f.text_area :timesincestarted %>
		  </p>
		  <p>
		    <%= f.label :ownershiptype %><br>
		    <%= options_for_select([['Pemilik', 1], ['Perusahaan', 2], ['Partnership', 3]], 1) %>
		  </p>
		  <p>
		    <%= f.label :revenueseachmonth %><br>
		    <%= f.text_field :revenueseachmonth%>
		  </p>
		  <p>
		    <%= f.label :creditcardpaymentratio %><br>
		    <%= f.text_field :creditcardpaymentratio%>
		  </p>

		  <p>
		    <%= f.submit %>
		  </p>
		<% end %>
	</body>
</html>
