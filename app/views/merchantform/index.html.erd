<!DOCTYPE html>
<html>
	<head>
		
	</head>
	<body>

		<%= form_for :article do |f| %>
		  <p>
		    <%= f.label :merchant_name %><br>
		    <%= f.text_field :merchant_name%>
		  </p>
		  <p>
		    <%= f.label :merchant_website %><br>
		    <%= f.text_field :merchant_website%>
		  </p>
		  <p>
		    <%= f.label :merchant_email %><br>
		    <%= f.text_field :merchant_email%>
		  </p>
		  <p>
		    <%= f.label :merchant_address %><br>
		    <%= f.text_field :merchant_address%>
		  </p>
		  <p>
		    <%= f.label :business_status %><br>
		    <%= options_for_select([['Milik Sendiri', 1], ['Milik Keluarga', 2], ['Kontrak', 3]], 1) %>
		  </p>
		  <p>
		    <%= f.label :goods %><br>
		    <%= f.text_area :goods %>
		  </p>
		  <p>
		    <%= f.label :goods_price %><br>
		    <%= f.text_area :goods_price %>
		  </p>
		  <p>
		    <%= f.label :ownership %><br>
		    <%= options_for_select([['Pemilik', 1], ['Perusahaan', 2], ['Partnership', 3]], 1) %>
		  </p>
		  <p>
		    <%= f.label :monthly_income %><br>
		    <%= f.text_field :monthly_income%>
		  </p>
		  <p>
		    <%= f.label :monthly_income_cc %><br>
		    <%= f.text_field :monthly_income_cc%>
		  </p>

		  <p> PIC Merchant Info </p>
		  <p>
		    <%= f.label :pic_name %><br>
		    <%= f.text_field :pic_name%>
		  </p>
		  <p>
		    <%= f.label :pic_address %><br>
		    <%= f.text_field :pic_name%>
		  </p>
		  <p>
		    <%= f.label :pic_email %><br>
		    <%= f.text_field :pic_email%>
		  </p>
		  <p>
		    <%= f.label :pic_phone %><br>
		    <%= f.text_field :pic_phone%>
		  </p>

		  <p> Merchant Owner Info </p>
		  <p>
		    <%= f.label :mo_name %><br>
		    <%= f.text_field :mo_name%>
		  </p>
		  <p>
		    <%= f.label :mo_address %><br>
		    <%= f.text_field :mo_name%>
		  </p>
		  <p>
		    <%= f.label :mo_email %><br>
		    <%= f.text_field :mo_email%>
		  </p>
		  <p>
		    <%= f.label :mo_phone %><br>
		    <%= f.text_field :mo_phone%>
		  </p>

		  <p> Bank Account Information </p>
		  <p>
		    <%= f.label :bank_name %><br>
		    <%= f.text_field :bank_name%>
		  </p>
		  <p>
		    <%= f.label :bank_acc_num %><br>
		    <%= f.text_field :bank_acc_num%>
		  </p>
		  <p>
		    <%= f.label :bank_acc_holder %><br>
		    <%= f.text_field :bank_acc_holder%>
		  </p>
		  <p>
		    <%= f.label :bank_acc_type %><br>
		    <%= f.text_field :bank_acc_type%>
		  </p>
 
		  <p>
		    <%= f.submit %>
		  </p>
		<% end %>
	</body>
</html>
