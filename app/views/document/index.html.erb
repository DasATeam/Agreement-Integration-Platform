<h3>Merchant: <%= @merchant.name %></h3>
<% @merchant_documents.each do |d| %>
  <h3><%= d.document_type.name %></h3>
  <% if d.path %>
    <%= image_tag d.path %>
  <% else %>
    <p>Belum ada file yang diupload</p>
  <% end %>
  <%= form_for :document do |f| %>
    <%= f.file_field d.id.to_s %><br>
    <%= f.submit 'Upload', :class => 'btn btn-primary' %>
  <% end %>
  <br>
<% end %>
